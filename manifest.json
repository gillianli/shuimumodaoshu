{
  "manifest_version": 3,
  "name": "魔导书水木版插件",
  "version": "1.0",
  "description": "帮助生成Tag",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [
		"storage",
		"activeTab",
		"scripting"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}
